{
  "name": "Immersa",
  "author": "Ertuğrul Çetin",
  "description": "A Web-based 3D Presentation Tool",
  "scripts": {
    "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
    "watch": "sh ./sh/copy_havok.sh && npx shadow-cljs watch app",
    "test": "npx shadow-cljs watch browser-test karma-test",
    "release": "sh ./sh/copy_havok.sh && npx shadow-cljs release app",
    "build-report": "npx shadow-cljs run shadow.cljs.build-report app target/build-report.html",
    "ci": "npx shadow-cljs compile karma-test && npx karma start --single-run --reporters junit,dots",
    "clean": "rm -rf .shadow-cljs resources/public/js/compiled target",
    "clean-all": "rm -rf .shadow-cljs resources/public/js/compiled node_modules package-lock.json target",
    "postinstall": "patch-package --exclude nothing"
  },
  "dependencies": {
    "@babylonjs/core": "^6.46.0",
    "@babylonjs/gui": "^6.46.0",
    "@babylonjs/loaders": "^6.46.0",
    "@babylonjs/materials": "^6.46.0",
    "@babylonjs/havok": "^1.3.1",
    "@clerk/clerk-react": "^4.30.5",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@phosphor-icons/react": "^2.0.15",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "earcut": "^2.2.4",
    "firebase": "^10.8.0",
    "highlight.js": "11.5.1",
    "p5": "^1.9.0",
    "progressbar.js": "^1.1.1",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babylonjs/inspector": "^6.41.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-cljs-test": "0.1.0",
    "karma-junit-reporter": "2.0.1",
    "patch-package": "^8.0.0",
    "shadow-cljs": "2.26.0"
  }
}
